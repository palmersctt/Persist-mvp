{
  "functions": {
    "pages/api/auth/[...nextauth].ts": {
      "maxDuration": 30,
      "env": {
        "TZ": "America/Los_Angeles"
      }
    },
    "pages/api/**/*.ts": {
      "maxDuration": 15,
      "env": {
        "TZ": "America/Los_Angeles"
      }
    }
  },
  "env": {
    "TZ": "America/Los_Angeles"
  },
  "rewrites": [
    {
      "source": "/api/auth/(.*)",
      "destination": "/api/auth/$1"
    }
  ]
}